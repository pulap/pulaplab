<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%.-3level] : %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="org.eclipse.jetty" level="OFF" />
    <logger name="org.eclipse.jetty.util" level="OFF" />
    <logger name="org.eclipse.jetty.server" level="OFF" />
    <logger name="org.eclipse.jetty.http" level="OFF" />
    <logger name="org.eclipse.jetty.io" level="OFF" />
    <logger name="org.eclipse.jetty.servlet" level="OFF" />
    <logger name="org.eclipse.jetty.webapp" level="OFF" />
    <logger name="org.eclipse.jetty.security" level="OFF" />
    <logger name="org.eclipse.jetty.deploy" level="OFF" />
    <logger name="org.eclipse.jetty.xml" level="OFF" />

    <root level="WARN">
        <appender-ref ref="STDOUT" />
    </root>

    <logger name="pulaplab" level="DEBUG" />
</configuration>